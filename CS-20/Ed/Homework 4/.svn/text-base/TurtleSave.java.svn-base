
public class TurtleSave {

    public int x, y;
    public double heading;
    
    public TurtleSave(int x, int y, double h)   {
        this.x = x;
        this.y = y;
        heading = h;
    }
    
    public void setPos(int x, int y)    {
    }
    
    public void setHeading(double heading) {
        this.heading = heading;
    }
    
    public int getX()   {
        return x;
    }
    
    public int getY()   {
        return y;
    }
    
    public double getHeading()  {
        return heading;
    }
    
}
