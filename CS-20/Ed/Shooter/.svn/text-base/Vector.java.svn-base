
public class Vector {

    public float x;
    public float y;
    
    public Vector(float x, float y) {
        this.x = x;
        this.y = y;
    }
    
    public static Vector normalize(Vector v)  {
        float mag = (v.x*v.x) + (v.y*v.y);
        v.x /= mag;
        v.y /= mag;
        
        return v;
    }
    
    public void setX(float x)   {
        this.x = x;
    }
    
    public void sety(float y)   {
        this.y = y;
    }
    
    public float getX() {
        return x;
    }
    
    public float getY() {
        return y;
    }
    
}
